{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BASE_URL": "@next_public_base_url",
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  },
  "functions": {
    "app/api/generate/route.ts": {
      "maxDuration": 60
    },
    "app/api/deploy/route.ts": {
      "maxDuration": 120
    },
    "app/api/github/push/route.ts": {
      "maxDuration": 60
    }
  }
}
